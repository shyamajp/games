{
  "name": "games",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=20 <21"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
